{
    "name": "zuruuh/blagues-api",
    "description": "PHP wrapper for blagues-api",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "zuruuh",
            "email": "ziadi.mail.pro@gmail.com",
            "homepage": "https://github.com/Zuruuh"
        }
    ],
    "require": {
        "php": ">=8.0",
        "php-http/discovery": "^1.19",
        "php-http/httplug": "^2.4",
        "psr/http-client-implementation": "*",
        "psr/http-factory": "^1.0",
        "symfony/serializer": "^4.0|^5.0|^6.0|^7.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.8",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-php-parser": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^9",
        "symfony/http-client": "^6.3",
        "symfony/var-dumper": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Zuruuh\\BlaguesApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zuruuh\\BlaguesApi\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
